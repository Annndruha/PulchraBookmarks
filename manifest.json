{
   "name": "Pulchra bookmarks",
   "description": "Pulchra bookmarks extention",
   "version": "0.0.2",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "manifest_version": 3,
   "minimum_chrome_version": "88",

   "chrome_url_overrides": {
      "newtab": "newtab.html"
   },

   "background": {
      "service_worker": "js/bottom-menu.js"
   },

   "permissions": ["activeTab",
      "bookmarks",
      "geolocation",
      "history",
      "notifications",
      "activeTab",
      "scripting",
      "storage",
      "tabs",
      "topSites",
      "unlimitedStorage",
      "webNavigation"]

}
