{
  "name": "Pulchra Bookmarks",
  "description": "Pulchra Bookmarks extension",
  "version": "0.9.10",
  "manifest_version": 3,
  "minimum_chrome_version": "96",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons":{
    "128": "images/branding/logo128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {},
  "permissions": [
    "storage",
    "unlimitedStorage",
    "bookmarks"
  ],
  "host_permissions": ["<all_urls>"]
}
