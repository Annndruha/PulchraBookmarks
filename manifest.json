{
   "name": "Pulchra bookmarks",
   "description": "Pulchra bookmarks extension",
   "version": "0.0.2",
   "manifest_version": 3,
   "minimum_chrome_version": "88",

   "chrome_url_overrides": {
      "newtab": "newtab.html"
   },

   "background": {
      "service_worker": "js/background.js"
   },

   "permissions": ["activeTab",
      "bookmarks",
      "history",
      "scripting",
      "storage",
      "tabs",
      "topSites",
      "unlimitedStorage",
      "webNavigation"]

}
