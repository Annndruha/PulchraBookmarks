{
  "name": "Pulchra bookmarks",
  "description": "Pulchra bookmarks extension",
  "version": "0.3.0",
  "manifest_version": 3,
  "minimum_chrome_version": "96",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons":{
    "128": "images/branding/logo128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {},
  "permissions": [
    "tabs",
    "bookmarks",
    "history",
    "storage",
    "topSites"
  ]
}
